{
    "manifest_version": 2,

    "name": "Search FB Name",
    "description": "This extension help you find FB accounts by name that display profile image",
    "version": "0.1",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab",
        "storage",
        "http://graph.facebook.com/*",
        "https://www.facebook.com/*",
        "https://code.jquery.com/*",
        "https://connect.facebook.net/*"
    ],
    "web_accessible_resources":[
        "style.css",
        "script.js",
        "fbscript.js"
    ],
    "content_security_policy":
        "script-src 'self' https://code.jquery.com https://connect.facebook.net; object-src 'self'"
}
